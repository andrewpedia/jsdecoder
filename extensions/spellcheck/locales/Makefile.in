DEFINES += -DAB_CD=$(AB_CD)

include $(topsrcdir)/config/rules.mk

DICTIONARY_FILES = $(strip $(wildcard $(LOCALE_SRCDIR)/hunspell/*.dic) $(wildcard $(LOCALE_SRCDIR)/hunspell/*.aff))

